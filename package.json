    "react-day-picker": "^9.4.1",
    "react-dom": "^18.2.0",
    "react-loader-spinner": "^6.1.6",
    "react-router-dom": "^6.28.0",
    "react-toastify": "^10.0.6"
  },
  "scripts": {
    "start": "cross-env REACT_APP_USE_MSW=true react-scripts start",
    "start:no-msw": "cross-env REACT_APP_USE_MSW=false BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "cypress": "concurrently npm:start:no-msw npm:cypress:open",
    "cypress:open": "cypress open"
  },
  "eslintConfig": {
    "plugins": [
      "cypress"
    ],
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:cypress/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/cypress": "^10.0.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.2.25",
    "@types/react-router-dom": "^5.3.3",
    "@types/testing-library__cypress": "^5.0.13",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "cypress": "^13.16.0",
    "eslint-plugin-cypress": "^3.5.0",
