{
  "name": "parking-website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@aws-amplify/auth": "^5.3.3",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@sentry/react": "^7.50.0",
    "@tanstack/react-query": "^4.29.5",
    "bulma": "^0.9.4",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3",
    "formik": "^2.2.9",
    "hibp": "12.0.1",
    "jwt-decode": "^3.1.2",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-day-picker": "^8.7.1",
    "react-dom": "^18.2.0",
    "react-loader-spinner": "^5.3.4",
    "react-router-dom": "^6.10.0",
    "react-toastify": "^9.1.2"
  },
  "scripts": {
    "start": "cross-env REACT_APP_USE_MSW=true react-scripts start",
    "start:no-msw": "cross-env REACT_APP_USE_MSW=false BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "cypress": "concurrently npm:start:no-msw npm:cypress:open",
    "cypress:open": "cypress open"
  },
  "eslintConfig": {
    "plugins": [
      "cypress"
    ],
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:cypress/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/testing-library__cypress": "^5.0.9",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "cypress": "^12.11.0",
    "eslint-plugin-cypress": "^2.13.2",
    "msw": "^1.2.1",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5"
  },
  "msw": {
    "workerDirectory": "public"
